<div id="welcome">
  <div class="container">
    <h1>Update BriefCases</h1>
    <hr/>
    <div class="buttons">
      <!--<a class="btn btn-info" href="/account/profile">Update my email</a>-->
      <!--<button class="btn btn-outline-info ml-2" @click="clickOpenDashboardModalButton()">List Briefcases</button>-->
      <a class="btn btn-info" href="/api/Maletines">List Briefcases</a>
    </div>
  </div>
  <div class="container">
		<div class="modal-body mx-3">
			<form method="POST" action="/api/Maletin/<%= Maleta.id %>">
				      <div class="modal-body mx-3">
				            <div class="md-form mb-5">
				               <label data-error="wrong" data-success="right" for="defaultForm-email">Name</label>
				                  <i class="fa fa-briefcase prefix grey-text"></i>
				                <input type="text" id="name" name="name" class="form-control validate" value="<%= Maleta.name %>" placeholder="@example" required="true">  
				            </div>

				      </div>
			          <div class="d-flex justify-content-center">
			              <button type="input" class="btn btn-info" value="Create"> Update</button>
			          </div>
			</form>
		</div>
  </div>



</div>
<%- /* Expose locals as `window.SAILS_LOCALS` :: */ exposeLocalsToBrowser() %>